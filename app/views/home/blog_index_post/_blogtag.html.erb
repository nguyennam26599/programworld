<div class="view-blog bg-white p-3">
  <div class="title-blog">
    <i class="far fa-chart-bar"></i>
    <span class="font-size-11em">Tags Feed</span>
  </div>
  <div>
    <% @posts.order(created_at: :desc).each do |post| %>
      <%= render partial: "home/post_index/topictag", locals: {post: post} %>
    <% end %>
    <%== render partial: 'users/shared/pagination', locals: {pagy: @pagy} %>
  </div>
</div>
